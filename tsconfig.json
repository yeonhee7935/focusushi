{
  "compilerOptions": {
    "target": "ES2021",
    "lib": [
      "ES2021",
      "DOM"
    ],
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "module": "ESNext",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowJs": false,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "types": []
  },
  "include": [
    "index.ts",
    "App.tsx",
    "src"
  ],
  "extends": "expo/tsconfig.base"
}
